{ 
    "chmod": "755",
    "directories": [
        "src"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": [
                "vendor/composer",
                "vendor/illuminate/support",
                "vendor/symfony/console",
                "vendor/symfony/polyfill-mbstring"
            ]
        },
        {
            "name": "autoload.php",
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "skype-logs",
    "output": "skype-logs.phar",
    "stub": true
}
